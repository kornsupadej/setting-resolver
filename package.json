{
  "name": "@kornsupadej/setting-resolver",
  "version": "0.0.0",
  "description": "My toolbox for project settings",
  "author": "Kornsupadej Suvarnakasa",
  "license": "MIT",
  "scripts": {
    "test": "mocha --no-package --config=./mocha.config.cjs",
    "test:watch": "mocha --no-package --config=./mocha.config.cjs --watch",
    "lint": "eslint \"**/*.{js,cjs}\" --fix",
    "lint:check": "eslint \"**/*.{js,cjs}\"",
    "format": "prettier \"**/*.{js,cjs,json,yml,yaml}\" --write",
    "format:check": "prettier \"**/*.{js,cjs,json,yml,yaml}\""
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "chai": "^5.2.0",
    "eslint": "^9.21.0",
    "mocha": "^11.1.0",
    "prettier": "^3.5.2",
    "sinon": "^19.0.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@vitest/eslint-plugin": "1.1.31",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-n": "^17.15.1",
    "globals": "^16.0.0",
    "merge-deep": "^3.0.3",
    "mixin-deep": "^2.0.1",
    "typescript-eslint": "^8.24.1"
  },
  "peerDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "prettier": {
      "optional": true
    }
  },
  "keywords": [
    "ast",
    "lint",
    "javascript",
    "ecmascript",
    "espree"
  ]
}
